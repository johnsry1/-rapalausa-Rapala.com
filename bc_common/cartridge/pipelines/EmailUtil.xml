<?xml version="1.0" encoding="UTF-8" ?>
<?demandware-pipeline version="2.0"?>

<pipeline group="util" type="job">
  <branch basename="_ANONYMOUS_BRANCH_1">
    <segment>
      <node>
        <text-node>
          <description>Copyright &#169; 2013 Priority Fulfillment Services, Inc. All rights reserved.</description>
        </text-node>
        <node-display x="0" y="0"/>
      </node>
    </segment>
  </branch>
  <branch basename="SendEmail">
    <segment>
      <node>
        <start-node call-mode="private" name="SendEmail" secure="false"/>
        <node-display x="2" y="0"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node pipelet-name="Script" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="true"/>
          <config-property key="OnError" value="PIPELET_ERROR"/>
          <config-property key="ScriptFile" value="util/CreateJobLog.ds"/>
          <key-binding alias="ScriptLog" key="ScriptLog"/>
          <key-binding alias="JobLog" key="jobLog"/>
          <key-binding alias="JobLog" key="JobLog"/>
          <key-binding alias="JobType" key="JobName"/>
          <key-binding alias="Debug" key="Debug"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="error">
          <transition target-connector="in1" target-path="./+1">
            <transition-display>
              <bend-point relative-to="source" x="2" y="0"/>
              <bend-point relative-to="target" x="2" y="0"/>
            </transition-display>
          </transition>
        </branch>
      </node>
      <transition target-connector="in1" target-path="./+1"/>
    </segment>
    <segment>
      <node>
        <join-node/>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node pipelet-name="Script" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="false"/>
          <config-property key="OnError" value="PIPELET_ERROR"/>
          <config-property key="ScriptFile" value="util/CreateJobMailSubject.ds"/>
          <key-binding alias="ScriptLog" key="ScriptLog"/>
          <key-binding alias="JobType" key="joblogname"/>
          <key-binding alias="MailSubject" key="MailSubject"/>
          <key-binding alias="hasErrors" key="hasErrors"/>
          <key-binding alias="hasWarns" key="hasWarns"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="error">
          <transition target-connector="in1" target-path="./+1">
            <transition-display>
              <bend-point relative-to="source" x="2" y="0"/>
              <bend-point relative-to="target" x="2" y="0"/>
            </transition-display>
          </transition>
        </branch>
      </node>
      <transition target-connector="in1" target-path="./+1"/>
    </segment>
    <segment>
      <node>
        <join-node/>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node pipelet-name="SendMail" pipelet-set-identifier="bc_api">
          <key-binding alias="dw.system.Site.current.preferences.custom.ete_systemEmailAddress" key="MailFrom"/>
          <key-binding alias="&quot;service/errorMail&quot;" key="MailTemplate"/>
          <key-binding alias="dw.system.Site.current.preferences.custom.ete_adminEmailAddress" key="MailTo"/>
          <key-binding alias="MailSubject" key="MailSubject"/>
          <key-binding alias="&quot;default&quot;" key="LocaleID"/>
          <key-binding alias="null" key="MailCC"/>
          <key-binding alias="null" key="MailBCC"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <end-node/>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="SendEmailSuccess">
    <segment>
      <node>
        <start-node call-mode="private" name="SendEmailSuccess" secure="false"/>
        <node-display x="5" y="0"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node pipelet-name="Script" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="true"/>
          <config-property key="OnError" value="PIPELET_ERROR"/>
          <config-property key="ScriptFile" value="util/CreateJobLogSuccess.ds"/>
          <key-binding alias="ScriptLog" key="ScriptLog"/>
          <key-binding alias="JobLog" key="jobLog"/>
          <key-binding alias="JobLog" key="JobLog"/>
          <key-binding alias="JobType" key="JobName"/>
          <key-binding alias="Debug" key="Debug"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="error">
          <transition target-connector="in">
            <transition-display>
              <bend-point relative-to="source" x="1" y="0"/>
            </transition-display>
          </transition>
          <segment>
            <node>
              <end-node/>
              <node-display orientation="horizontal" x="1" y="0"/>
            </node>
          </segment>
        </branch>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node pipelet-name="Script" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="false"/>
          <config-property key="OnError" value="PIPELET_ERROR"/>
          <config-property key="ScriptFile" value="util/CreateJobMailSubjectSuccess.ds"/>
          <key-binding alias="ScriptLog" key="ScriptLog"/>
          <key-binding alias="JobType" key="joblogname"/>
          <key-binding alias="MailSubject" key="MailSubject"/>
          <key-binding alias="hasErrors" key="hasErrors"/>
          <key-binding alias="hasWarns" key="hasWarns"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
        <branch basename="b3" source-connector="error">
          <transition target-connector="in1" target-path="./+1">
            <transition-display>
              <bend-point relative-to="source" x="2" y="0"/>
              <bend-point relative-to="target" x="2" y="0"/>
            </transition-display>
          </transition>
        </branch>
      </node>
      <transition target-connector="in1" target-path="./+1"/>
    </segment>
    <segment>
      <node>
        <join-node/>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node pipelet-name="SendMail" pipelet-set-identifier="bc_api">
          <key-binding alias="dw.system.Site.current.preferences.custom.ete_systemEmailAddress" key="MailFrom"/>
          <key-binding alias="&quot;service/errorMail&quot;" key="MailTemplate"/>
          <key-binding alias="dw.system.Site.current.preferences.custom.ete_adminEmailAddress" key="MailTo"/>
          <key-binding alias="MailSubject" key="MailSubject"/>
          <key-binding alias="&quot;default&quot;" key="LocaleID"/>
          <key-binding alias="null" key="MailCC"/>
          <key-binding alias="null" key="MailBCC"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <end-node/>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
</pipeline>

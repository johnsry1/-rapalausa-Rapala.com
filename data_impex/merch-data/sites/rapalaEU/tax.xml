<?xml version="1.0" encoding="UTF-8"?>
<tax xmlns="http://www.demandware.com/xml/impex/tax/2007-02-14">
    <tax-classes>
        <tax-class class-id="Standard" default="true">
            <display-name>Standard Tax Class</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="default">
            <display-name>Default Tax Class</display-name>
            <description/>
        </tax-class>

        <tax-class class-id="exempt">
            <display-name>Exempt Tax Class</display-name>
            <description>No Tax Applied</description>
        </tax-class>

    </tax-classes>
    <tax-jurisdictions>
        <tax-jurisdiction jurisdiction-id="DE">
            <display-name>Germany</display-name>
            <address-mappings>
                <address-mapping key1="DE" key2="-"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="GB">
            <display-name>Great Britain / United Kingdom</display-name>
            <address-mappings>
                <address-mapping key1="GB" key2="-"/>
            </address-mappings>
        </tax-jurisdiction>

        <tax-jurisdiction jurisdiction-id="default" default="true">
            <display-name>Default</display-name>
        </tax-jurisdiction>

    </tax-jurisdictions>
    <tax-rates>
        <tax-rate jurisdiction-id="GB" class-id="exempt">0</tax-rate>

        <tax-rate jurisdiction-id="default" class-id="exempt">0</tax-rate>

        <tax-rate jurisdiction-id="DE" class-id="exempt">0</tax-rate>

        <tax-rate jurisdiction-id="GB" class-id="default">0.24</tax-rate>

        <tax-rate jurisdiction-id="default" class-id="default">0</tax-rate>

        <tax-rate jurisdiction-id="DE" class-id="default">0.2</tax-rate>

        <tax-rate jurisdiction-id="GB" class-id="Standard">0.24</tax-rate>

        <tax-rate jurisdiction-id="default" class-id="Standard">0</tax-rate>

        <tax-rate jurisdiction-id="DE" class-id="Standard">0.2</tax-rate>

    </tax-rates>
</tax>

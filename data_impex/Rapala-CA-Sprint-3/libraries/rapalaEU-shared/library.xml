<?xml version="1.0" encoding="UTF-8"?>
<library xmlns="http://www.demandware.com/xml/impex/library/2006-10-31" library-id="rapalaEU-shared">
    <content content-id="mail-chimp-footer">
        <display-name xml:lang="x-default">Footer Email</display-name>
        <description xml:lang="x-default">Mail Chimp Footer Email</description>
        <online-flag>true</online-flag>
        <searchable-flag>false</searchable-flag>
        <custom-attributes>
          <custom-attribute attribute-id="body" xml:lang="x-default">&lt;!-- Begin Mailchimp Signup Form --&gt;

            &lt;div id=&quot;mc_embed_signup&quot;&gt;
            &lt;form action=&quot;https://rapala.us7.list-manage.com/subscribe/post?u=23c0b7839e54f3fa90bde1266&amp;amp;id=1559c1f9ba&quot; method=&quot;post&quot; id=&quot;mc-embedded-subscribe-form&quot; name=&quot;mc-embedded-subscribe-form&quot; class=&quot;email-subscribe validate&quot; target=&quot;_blank&quot; novalidate&gt;
            &lt;div id=&quot;mc_embed_signup_scroll&quot;&gt;
            &lt;input type=&quot;email&quot; value=&quot;&quot; name=&quot;EMAIL&quot; class=&quot;email&quot; id=&quot;mce-EMAIL&quot; placeholder=&quot;10% Off Your First Order*&quot; onfocus=&quot;this.placeholder=''&quot; onblur=&quot;this.placeholder='10% Off Your First Order*'&quot; required&gt;
            &lt;!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups--&gt;
            &lt;div style=&quot;position: absolute; left: -5000px;&quot; aria-hidden=&quot;true&quot;&gt;&lt;input type=&quot;text&quot; name=&quot;b_23c0b7839e54f3fa90bde1266_1559c1f9ba&quot; tabindex=&quot;-1&quot; value=&quot;&quot;&gt;&lt;/div&gt;
            &lt;div class=&quot;clear&quot;&gt;&lt;input type=&quot;submit&quot; value=&quot;Subscribe&quot; name=&quot;subscribe&quot; id=&quot;mc-embedded-subscribe&quot; class=&quot;button&quot;&gt;&lt;/div&gt;
            &lt;/div&gt;
            &lt;/form&gt;
            &lt;/div&gt;

            &lt;!--End mc_embed_signup--&gt;</custom-attribute>
          </custom-attributes>
    </content>
    <content content-id="footer-signup-tooltip">
        <display-name xml:lang="x-default">Footer Signup Tooltip</display-name>
        <online-flag>true</online-flag>
        <searchable-flag>false</searchable-flag>
        <custom-attributes>
          <custom-attribute attribute-id="showInMainMenu">false</custom-attribute>
          <custom-attribute attribute-id="body" xml:lang="x-default">&lt;b&gt;Rapala &amp; Respected Brands Emails&lt;/b&gt;

            &lt;br&gt;&lt;br&gt;Sign up to receive regular email news, tips, product info, Pro picks and tips, promotions, special offers and more from Rapala and our Respected Brands covering fresh water, saltwater and ice fishing, including Rapala, VMC, Sufix, Storm, Luhr-Jensen, Terminator, Blue Fox, StrikeMaster.

            &lt;br&gt;&lt;br&gt;You can change your preferences and your email address and unsubscribe at any time through the links at the bottom of every email.

            &lt;br&gt;&lt;br&gt;Please click here for more information on our &lt;a title=&quot;Privacy&quot; href=&quot;$httpsURL('Page-Show','cid','customer-privacy-security')$&quot;&gt;privacy practices.&lt;/a&gt;</custom-attribute>
        </custom-attributes>
    </content>
</library>
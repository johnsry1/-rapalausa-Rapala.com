&lt;script type="text/javascript"&gt;&#13;
&#13;
 	  	$('.pt_customerservice #customercontactus textarea[maxlength]').each(function(){&#13;
			charcount($(this));&#13;
			// trigger the keydown event so that any existing character data is calculated&#13;
		}).on('keyup keypress', function(){&#13;
			charcount($(this));&#13;
		});&#13;
		&#13;
		function charcount($this){&#13;
			var characterLimit = "1000";&#13;
			var charRemains = characterLimit - $this.val().trim().length;&#13;
                        var enteredchar = $this.val().trim().length;&#13;
			var charCountContainer='';&#13;
			var charCountHtml = enteredchar + '/' + characterLimit ;&#13;
			if($this.hasClass("contactus_comment")){&#13;
				if ($this.next('div.char-count').length === 0) {&#13;
					charCountContainer = $('&lt;div class="char-count contact-us"/&gt;').insertAfter($this);&#13;
				}&#13;
				$this.next('div.char-count').html(charCountHtml);&#13;
			}else {&#13;
				if ($this.prev('div.char-count').length === 0) {&#13;
					charCountContainer = $('&lt;div class="char-count contact-us"/&gt;').insertBefore($this);&#13;
				}&#13;
				$this.prev('div.char-count').html(charCountHtml);&#13;
			}&#13;
		}&#13;
&lt;/script&gt;</custom-attribute>

<isinclude template="util/modules"/>

<isif condition="${pdict.countryValue === 'GB' || pdict.countryValue === 'IE'}">
    <isscript>
        help = {
            cid: "help-telephone",
            label: {
                property: "?",
                file: "checkout"
            }
        }
    </isscript>
	<isinputfield formfield="${pdict.fieldObject.formfield}" type="${pdict.fieldObject.type}" help="${help}"/>
</isif>

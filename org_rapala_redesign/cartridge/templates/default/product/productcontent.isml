										<span class="addedto-img"></span>
										<span class="addedto-text"><isprint value="${Resource.msg('product.addedtocart','product',null)}"/></span>
									</div>
								</div>
								<div class="addTo-cart-section">
									<input type="number" class="input-text quantityinput" name="Quantity" id="Quantity" size="2" maxlength="3" value="${Number(empty(pdict.CurrentHttpParameterMap.Quantity.stringValue) ? 1 : pdict.CurrentHttpParameterMap.Quantity.stringValue).toFixed()}" data-available="${availableCount}"/>
									<isif condition="${pdict.CurrentHttpParameterMap.isCart.stringValue == 'true'}">
										<input type="hidden" value="${pdict.CurrentHttpParameterMap.lineItemID.stringValue}" name="lineItemId" class="line-itemid"/>
										<button onClick="googleAnalyticsEvents.addProduct('${pdict.Product.ID}');"  type="button"  title="${Resource.msg('global.updatecart','locale',null)}"  value="${Resource.msg('global.updatecart','locale',null)}"  class="button-fancy-large add-to-cart addtocartbutton">${Resource.msg('global.cartupdate','locale',null)}</button>
									<iselse/>
										<button onClick="googleAnalyticsEvents.addProduct('${pdict.Product.ID}');" id="add-to-cart" type="button" title="${buttonTitle}" value="${buttonTitle}" class="button-fancy-large add-to-cart addtocartbutton recommendation_cart" data-gtmdata="${JSON.stringify(tagManager.getProductData(pdict.Product.ID))}">${buttonTitle}</button>
									</isif>
								</div>
							</div>
						<iselse/>
							<isscript>
								var pvm : dw.catalog.ProductVariationModel = pdict.Product.getVariationModel();
								var it : dw.util.Iterator = pvm.getProductVariationAttributes().iterator();
								var array : Array = [];
								var options = '';
								var requiredOptions = '';
								while( it.hasNext() ) {
									var text : dw.object.ObjectAttributeDefinition = it.next();
									array.push( text.displayName );
								}
								options = array.join(', ');
								var lastIndex = options.lastIndexOf(',');
								if( lastIndex > 0 && options.length > 1 && array.length > 1) {
								 requiredOptions = options.substr(0,lastIndex) + ' ' + dw.web.Resource.msg('product.attributedivider', 'product', null) + options.substr(lastIndex+1, options.length);
								} else {
								 requiredOptions = options;
								}
								var buttonTitleDisabledSelectVariation = StringUtils.format(dw.web.Resource.msg('product.missingval','product', null), requiredOptions);
							</isscript>
							<div class="addtocart disabled">
								<div class="inventory">
									<div class="quantity">
										<span class="label"><isprint value="${Resource.msg('global.quantity','locale',null)}" /></span>
									</div>
								</div>
								<div class="addedto-cartoverlay">
									<div class="addedto-sucess-wrapper">
										<span class="addedto-img"></span>
										<span class="addedto-text"><isprint value="${Resource.msg('product.addedtocart','product',null)}"/></span>
									</div>
								</div>
								<div class="addTo-cart-section">
									<input type="number" class="input-text quantityinput" name="Quantity" id="Quantity" size="2" maxlength="3" value="${Number(empty(pdict.CurrentHttpParameterMap.Quantity.stringValue) ? 1 : pdict.CurrentHttpParameterMap.Quantity.stringValue).toFixed()}" data-available="${availableCount}" disabled/>
									<button onClick="googleAnalyticsEvents.addProduct('${pdict.Product.ID}')" id="add-to-cart" type="button" title="${buttonTitleDisabledSelectVariation}" value="${buttonTitleDisabledSelectVariation}" data-gtmdata="${JSON.stringify(tagManager.getProductData(pdict.Product.ID))}" class="button-fancy-large  add-to-cart-disabled addtocartbutton disabled"<isprint value="${disabledAttr}" encoding="off"/>>${buttonTitle}</button>
								</div>
							</div>
						</isif>
					</div><!--  end details block -->
				</div>
			</fieldset>
		</form>


<script type="text/javascript">
    $('.pt_customerservice #customercontactus textarea[maxlength]').each(function(){
        charcount($(this));
        // trigger the keydown event so that any existing character data is calculated
    }).on('keyup keypress', function(){
        charcount($(this));
    });

    function charcount($this){
        var characterLimit = "1000";
        var charRemains = characterLimit - $this.val().trim().length;
        var enteredchar = $this.val().trim().length;
        var charCountContainer='';
        var charCountHtml = enteredchar + '/' + characterLimit ;

        if ($this.next('div.char-count').length === 0) {
            charCountContainer = $('<div class="char-count contact-us"/>').insertAfter($this);
        }

        $this.next('div.char-count').html(charCountHtml);
    }
</script>

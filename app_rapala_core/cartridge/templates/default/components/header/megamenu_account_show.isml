<div class="menulist">
	<span class="sub-category-section-1">
		<isif condition="${pdict.CurrentCustomer.registered}">
			<a class="level-1" href="${URLUtils.https('Login-Logout')}">
				<isprint value="${String.toUpperCase(Resource.msg('global.logout','locale',null))}"/>
			</a>
		<iselse>
			<a class="level-1" href="${URLUtils.https('Login-Show')}">
				${Resource.msg('global.login.create.account','locale',null)}
			</a>
		</isif>
 	</span>
</div>